{
    "name": "fortum-rost",
    "version": "0.0.0",
    "description": "Fortum ROST v1.0.0 Polymer Web Application.",
    "private": true,
    "main": "server",
    "engines": {
        "node": ">=6.0.0"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-3",
            "stage-0",
            "babel-polyfill"
        ],
        "plugins": [],
        "sourceMaps": "inline"
    },
    "scripts": {
        "deploy": "VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",.,-]//g') && VERSION=$(printf 'v%s'$VERSION) && gcloud app deploy ./platform.yaml --project=prd-use-rost-fortum --version=$VERSION --no-promote",
        "postdeploy": "npm version prerelease -m 'Bumped to %s' --git-tag-version=true",
        "webrew": "./bin/webrew",
        "deploy:dev": "gcloud preview app deploy environment.yaml --project=dev-use-rost-fortum --version=untran",
        "deploy:stg": "gcloud preview app deploy environment.yaml --project=stg-use-rost-fortum --version=untran",
        "prestart": "echo pre start!",
        "start": "npm run gulp & npm run serve",
        "serve": "firebase serve --port=8080",
        "koa": "node --require babel-register ./server/koa",
        "poststart": "echo post start!",
        "prestop": "echo pre stop!",
        "stop": "killall node | exit 0",
        "clean": "rm -r ./build",
        "poststop": "echo post stop!",
        "webrew-build": "babel --no-babelrc ./  --out-dir ./build --ignore www,client,build,node_modules --presets=es2015,stage-3,stage-0,babel-polyfill --plugins=transform-runtime,babel-root-import --source-maps=inline",
        "test": "mocha --watch --require babel-polyfill --compilers js:babel-register ./**/*.test.js",
        "gulp": "./node_modules/gulp-cli/bin/gulp.js --gulpfile=./gulp/gulp.babel.js --cwd=./",
        "preversion": "echo pre version!",
        "version": "echo version!",
        "postversion": "echo post version!"
    },
    "dependencies": {
        "@angular/common": "^2.0.0-rc.1",
        "@angular/compiler": "^2.0.0-rc.1",
        "@angular/core": "^2.0.0-rc.1",
        "@angular/http": "^2.0.0-rc.1",
        "@angular/platform-browser": "^2.0.0-rc.1",
        "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
        "@angular/router": "^2.0.0-rc.1",
        "babel-eslint": "^6.0.4",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-systemjs": "^6.6.5",
        "babel-preset-es2015": "^6.6.0",
        "body-parser": "^1.15.0",
        "browser-sync": "^2.12.5",
        "chai": "^3.5.0",
        "connect": "^3.4.1",
        "connect-modrewrite": "^0.9.0",
        "css-mqpacker": "^5.0.1",
        "del": "^2.2.0",
        "eslint": "^2.10.2",
        "express": "^4.13.4",
        "firebase-tools": "^3.2.1",
        "gulp-babel": "^6.1.2",
        "gulp-concat": "^2.6.0",
        "gulp-font": "^1.0.3",
        "gulp-polymer-babel": "^1.0.0",
        "gulp-polymer-postcss": "^1.0.0",
        "gulp-postcss": "^6.1.0",
        "gulp-rename": "^1.2.2",
        "handlebars": "^4.0.5",
        "jsonparse": "^1.2.0",
        "mocha": "^2.4.5",
        "morgan": "^1.7.0",
        "postcss-apply": "^0.2.0",
        "postcss-cssnext": "^2.5.2",
        "postcss-custom-media": "^5.0.1",
        "postcss-custom-properties": "^5.0.1",
        "postcss-nested": "^1.0.0",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.6",
        "serve-index": "^1.7.3",
        "serve-static": "^1.10.2",
        "systemjs": "^0.19.27",
        "through2": "^2.0.1",
        "traverse": "^0.6.6",
        "ua-parser-js": "^0.7.10",
        "vinyl-source-stream": "^1.1.0",
        "webrew-helpers-log": "0.0.7",
        "zone.js": "^0.6.12",
        "autoprefixer": "^6.6.0",
        "babel-polyfill": "^6.20.0",
        "babel-register": "^6.18.0",
        "chalk": "^1.1.3",
        "cross-spawn": "^5.0.1",
        "gulp": "github:gulpjs/gulp#4.0",
        "gulp-cli": "^1.2.2",
        "gulp-newer": "^1.3.0",
        "http2": "^3.3.6",
        "inquirer": "^2.0.0",
        "js-yaml": "^3.7.0",
        "koa": "^2.0.0-alpha.7",
        "koa-router": "^7.0.1",
